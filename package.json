{
  "name": "habit-tracker",
  "version": "1.0.0",
  "description": "A minimalist habit tracker that suggests random habits to build consistency",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "lint": "eslint .",
    "test": "jest",
    "build": "echo 'No build step required for this application'",
    "docker:build": "docker build -t habit-tracker .",
